@{
    ViewBag.Title = "Manage SubAdmin";
    Layout = "~/Views/Shared/_SuperAdminLayout.cshtml";
}
@section Styles{

    <link rel="stylesheet" href="~/Content/business-panel/css/vendor/select2-bootstrap.min.css" />
    <script src="https://maps.googleapis.com/maps/api/js?key=AIzaSyDj6D3QtBy7hfNpDe0Gqy2OjBCxrGF0d5k&callback=initMap" async defer></script>
    <style>
        .ClassCountryState {
            cursor: no-drop;
        }

        .subadminIcon {
            width: 75px;
            height: 75px;
        }

        .subadminIconPreview {
            border: 2px dashed #cecece;
            height: 75px;
            width: 75px;
            margin: 5px 0px;
            padding: 5px;
        }
    </style>

}

