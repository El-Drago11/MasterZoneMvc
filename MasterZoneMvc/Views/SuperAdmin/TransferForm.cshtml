
@{
    ViewBag.Title = "TransferForm";
    Layout = "~/Views/Shared/_SuperAdminLayout.cshtml";
    long customFormId = ViewBag.CustomFormId;
}


@section Styles{

    <style>
        

        .business-logo {
            height: 50px;
            width: 50px;
            object-fit: contain
        }
    </style>
}

<input type="hidden" id="hiddenCustomFormId" value="@ViewBag.CustomFormId" />
<div class="wrapper">

    
    <!-- Content Wrapper. Contains page content -->
    <div class="content-wrapper">
        <!-- Content Header (Page header) -->
        <section class="content-header">
            <div class="container-fluid">
                <div class="row mb-2">
                    <div class="col-sm-6">
                        <h1 id="ManagePageTextCertificate">Manage Transform Form</h1>
                    </div>
                    <div class="col-sm-6">
                        <ol class="breadcrumb float-sm-right">
                            <li class="breadcrumb-item"><a href="/SuperAdmin/Dashboard">Home</a></li>
                            <li class="breadcrumb-item active" id="pageTextchange">Manage Tansfer Form</li>
                        </ol>
                    </div>
                </div>
            </div><!-- /.container-fluid -->
        </section>




        <section class="content" id="CertificateAdd">
            <div class="container-fluid">
               

                <div class="row" id="AssignedBusinessOwnerSection">
                    <div class="col-sm-12">
                        <!-- Default box -->
                        <div class="card card-default">
                            <div class="card-header">
                                <h3 class="card-title">Assigned Business Owners</h3><br /><hr />
                                <div id="FormName"> </div>
                            </div>
                            <!-- /.card-header -->
                            <div class="card-body" style="max-height:350px; overflow-y:scroll;">
                                <div class="row">
                                    <div id="assignedBusinessData" class="col-sm-12">
                                        <table class="table table-bordered table-hover">
                                            <thead>
                                                <tr>
                                                    <th>S.No</th>
                                                    <th>MasterID</th>
                                                    <th>Business Name</th>
                                                    <th>Owner</th>
                                                    @*<th>Category</th>*@
                                                    @*<th>Sub-Category</th>*@
                                                    <th>Action</th>
                                                </tr>
                                            </thead>
                                            <tbody>
                                            </tbody>
                                        </table>
                                    </div>
                                </div>
                            </div>

                            <div class="card-footer">
                            </div>
                        </div>
                        <!-- /.card -->
                    </div>
                </div>

                <div class="row" id="AssignCertificateProfileSection">
                    <div class="col-sm-12">
                        <!-- Default box -->
                        <div class="card card-default">
                            <div class="card-header">
                                <h3 class="card-title">Assign Certification Profile</h3>
                            </div>
                            <!-- /.card-header -->
                            <div class="card-body">
                                <div class="row">
                                    <div class="col-md-6 ">
                                        <div class="form-group d-flex flex-column">
                                            @*<label>Search<span class="text-danger"></span></label>*@
                                            <input type="search" id="txtSearchBusiness" class="border form-control" placeholder="Search...">
                                            <div class="error-class" id="error_txtSearchBusiness"></div>
                                        </div>
                                    </div>
                                    <div class="col-sm-6">
                                        <button type="button" id="btnSearchBusiness" class="btn btn-success" onclick="getBusinessOnwersBySearch();">Search</button>
                                        <button type="button" id="btnClearBusinessCertificate" class="btn btn-default" onclick="resetAssignCertificateSection();">Clear</button>
                                    </div>
                                </div>
                                <div class="row">
                                    <div id="searchedBusinessOwnersList" class="row w-100"></div>
                                    <div class="col-sm-12 text-center"><a id="btnSearchBusinessShowMore" class="btn btn-outline-primary d-none" href="javascript:getBusinessOnwersBySearch(false)">Show More</a></div>
                                </div>
                            </div>

                            <div class="card-footer">
                            </div>
                        </div>
                        <!-- /.card -->
                    </div>
                </div>
            </div>

        </section>
    </div>
</div>


@section Scripts{


    @Html.Partial("/Views/Shared/ScriptPartials/SuperAdminPanel/_SuperAdminTransferFormScriptPartial.cshtml")
}
