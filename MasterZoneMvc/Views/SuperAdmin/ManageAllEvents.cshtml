
@{
    ViewBag.Title = "Manage Events";
    Layout = "~/Views/Shared/_SuperAdminLayout.cshtml";
}


@section Styles{

}

<!-- Content Wrapper. Contains page content -->
<div class="content-wrapper">
    <!-- Content Header (Page header) -->
    <section class="content-header">
        <div class="container-fluid">
            <div class="row mb-2">
                <div class="col-sm-6">
                    <h1>Manage Events</h1>
                </div>
                <div class="col-sm-6">
                    <ol class="breadcrumb float-sm-right">
                        <li class="breadcrumb-item"><a href="/SuperAdmin/Dashboard">Dashboard</a></li>
                        <li class="breadcrumb-item active">Manage Events</li>
                    </ol>
                </div>
            </div>
        </div><!-- /.container-fluid -->
    </section>

    <!-- View content -->
    <section class="content" id="sectionViewAllEvents">
        <div class="container-fluid">
            <div class="row">
                <div class="col-12">
                    <div class="card">
                        <div class="card-header">
                            <div class="row">
                                <div class="col-sm-9 d-flex align-items-center">
                                    <h3 class="card-title">View Events</h3>
                                </div>
                                <div class="col-sm-3 d-flex justify-content-end">
                                </div>
                            </div>
                        </div>
                        <!-- /.card-header -->
                        <div class="card-body">
                            <div class="row">
                                <div class="col-sm-12">
                                    <span class="text-muted font-weight-bold">@Resources.SuperAdminPanel.HomePageVisibilityDefination</span>
                                </div>
                            </div>

                            <table id="tblAllEvents" class="table table-bordered table-hover">
                                <thead>
                                    <tr>
                                        <th>#</th>
                                        <th>HPV</th>
                                        <th>Business</th>
                                        <th>Event</th>
                                        <th>Description</th>
                                        <th>Price</th>
                                        <th>Start Date</th>
                                        <th>End Date</th>
                                        <th>Event Archived</th>
                                    </tr>
                                </thead>
                                <tbody>
                                </tbody>
                                <tfoot>
                                    <tr>
                                        <th>#</th>
                                        <th>HPV</th>
                                        <th>Business</th>
                                        <th>Event</th>
                                        <th>Description</th>
                                        <th>Price</th>
                                        <th>Start Date</th>
                                        <th>End Date</th>
                                        <th>Event Archived</th>
                                    </tr>
                                </tfoot>
                            </table>
                        </div>
                        <!-- /.card-body -->
                    </div>
                    <!-- /.card -->
                    <!-- /.card -->
                </div>
                <!-- /.col -->
            </div>
            <!-- /.row -->
        </div>
        <!-- /.container-fluid -->
    </section>
    <!-- /. View content -->
    <!-- Main content -->
    <!-- /.content -->
</div>
<!-- /.content-wrapper -->

@section Scripts {

    @Html.Partial("/Views/Shared/ScriptPartials/SuperAdminPanel/_ManageAllEventsScriptPartial.cshtml")
}

