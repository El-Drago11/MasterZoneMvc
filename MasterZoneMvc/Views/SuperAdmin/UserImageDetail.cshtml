@{
    ViewBag.Title = "User Images";
    Layout = "~/Views/Shared/_SuperAdminLayout.cshtml";
    long _UserLoginId = ViewBag.UserLoginId;
}


@section Styles{
    <style>
        .previewImage {
            height: 185px;
            width: 160px;
        }
    </style>
}

<div class="content-wrapper">
    <!-- Content Header (Page header) -->
    <div class="content-header">
        <div class="container-fluid">
            <div class="row mb-2">
                <div class="col-sm-6">
                    <h1 class="m-0"> View Images</h1>
                </div><!-- /.col -->
                <div class="col-sm-6">
                    <ol class="breadcrumb float-sm-right">
                        <li class="breadcrumb-item"><a href="/SuperAdmin/StudentDetail?userLoginId=@_UserLoginId" class=" btn bg-gradient-navy">Back</a></li>

                    </ol>
                </div><!-- /.col -->
            </div><!-- /.row -->
        </div><!-- /.container-fluid -->
    </div>
    <section class="content">
        <div class="container-fluid">
            <input type="hidden" id="userLoginId" value="@_UserLoginId" />
            <div class="card" style="width: 100%">

                <div class="card-body">
                    <div class="tab-content">

                        <div class="row" id="ViewUserImageDetail">
                        </div>

                    </div>
                </div>
            </div>


        </div>
    </section>
</div>




@section Scripts{

    @Html.Partial("~/Views/Shared/ScriptPartials/SuperAdminPanel/_UserImageScriptPartial.cshtml")
    <script src="~/Content/lib/StylishAlert/iao-alert.jquery.js"></script>
    <script src="https://unpkg.com/sweetalert/dist/sweetalert.min.js"></script>
}

