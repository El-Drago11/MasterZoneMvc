
@{
    ViewBag.Title = "MyBooking";
    Layout = "~/Views/Shared/_BusinessAdminLayout.cshtml";
}
@section Styles{
    <style>
        .Image {
            width: 50px;
            margin-left: 17px;
        }

        .nav-tabs .nav-item {
            margin-bottom: 8px;
            margin-left: 139px;
            font-weight: bold;
            font-size: larger;
        }

        .nav-tabs .nav-link.active::before {
            content: " ";
            background: #00365a;
            color: #fff;
            border-radius: 10px;
            position: absolute;
            width: calc(100% - 1rem);
            height: 5px;
            top: 37px;
            left: 50%;
            transform: translateX(-50%);
        }

        .nav-link {
            cursor: pointer;
        }
    </style>
}

<input type="hidden" id="hiddenBusinessbranchLoginId" value="@ViewBag.BusinessBranchLoginId" />
<main>
    <div class="container-fluid">
        <div class="row">
            <div class="col-12">
                <div class="row">

                    <h1 class="pageHeadingText" id="pageTextchange">My Booking </h1>
                    
                </div>



                <div class="separator mb-5"></div>
            </div>
        </div>
        <div class="row">
            <div class="col-12">
                <div class="row">

                    <ul class="nav nav-tabs custom-tab-bar mtab-3jrjfj">
                        <li id="class_navtab" class="nav-item">
                            <a class="nav-link text-dark active" data-toggle="tab" data-target="#fav_Classes" role="tab">Classes</a>
                        </li>
                        <li id="event_navtab" class="nav-item">
                            <a class="nav-link text-dark" data-toggle="tab" data-target="#fav_Event" role="tab">Event</a>
                        </li>
                        <li id="training_navtab" class="nav-item">
                            <a class="nav-link text-dark" data-toggle="tab" data-target="#fav_Training" role="tab">Training</a>
                        </li>
                        <li id="plan_navtab" class="nav-item">
                            <a class="nav-link text-dark" data-toggle="tab" data-target="#fav_Plan" role="tab">Plan/Package</a>
                        </li>
                        <li id="course_navtab" class="nav-item">
                            <a class="nav-link text-dark" data-toggle="tab" data-target="#fav_Course" role="tab">Courses</a>
                        </li>
                    </ul>
                </div>



                <div class="separator mb-5"></div>
            </div>
        </div>
        <div class="tab-content">

            <div id="fav_Classes" class="tab-pane  fade active show p-3" role="tabpanel">
                <div class="row mb-4">
                    <div class="col-12 mb-4">
                        <div class="card">
                            <div class="card-body">
                                <table id="tblClassBookingDetails" class="data-table">
                                    <thead>
                                        <tr>
                                            <th>Sno.</th>
                                            <th>OrederId</th>
                                            <th>Class Name</th>
                                            <th>Price</th>
                                            <th>Student Name</th>
                                            <th>Date</th>
                                            <th>Action</th>

                                        </tr>
                                    </thead>
                                    <tbody></tbody>
                                </table>
                            </div>
                        </div>
                    </div>
                </div>

            </div>

            <div id="fav_Event" class="tab-pane fade p-3" role="tabpanel">
                <div class="row mb-4">
                    <div class="col-12 mb-4">
                        <div class="card">
                            <div class="card-body">
                                <table id="tblEventBookingDetails" class="data-table">
                                    <thead>
                                        <tr>
                                            <th>Sno.</th>
                                            <th>OrderId</th>
                                            <th>Name</th>
                                            <th>Price</th>
                                            <th>Student Name</th>
                                            <th>Date</th>
                                            <th>Action</th>

                                        </tr>
                                    </thead>
                                    <tbody></tbody>
                                </table>
                            </div>
                        </div>
                    </div>
                </div>

            </div>

            <div id="fav_Training" class="tab-pane fade p-3" role="tabpanel">
                <div class="row mb-4">
                    <div class="col-12 mb-4">
                        <div class="card">
                            <div class="card-body">
                                <table id="tblTrainingTrainingBookingDetail" class="data-table">
                                    <thead>
                                        <tr>
                                            <th>Sno.</th>
                                            <th>OrederId</th>
                                            <th>Name</th>
                                            <th>Price</th>
                                            <th>Student Name</th>
                                            <th>Date</th>
                                            <th>Action</th>

                                        </tr>
                                    </thead>
                                    <tbody></tbody>
                                </table>
                            </div>
                        </div>
                    </div>
                </div>

            </div>

            <div id="fav_Plan" class="tab-pane fade p-3" role="tabpanel">
                <div class="row mb-4">
                    <div class="col-12 mb-4">
                        <div class="card">
                            <div class="card-body">
                                <table id="tblPlanBookingDetails" class="data-table">
                                    <thead>
                                        <tr>
                                            <th>Sno.</th>
                                            <th>OrederId</th>
                                            <th> Name</th>
                                            <th> Price</th>
                                            <th>Student Name</th>
                                            <th>Date</th>
                                            <th>Action</th>

                                        </tr>
                                    </thead>
                                    <tbody></tbody>
                                </table>
                            </div>
                        </div>
                    </div>
                </div>

            </div>

            <div id="fav_Course" class="tab-pane  fade  p-3" role="tabpanel">
                <div class="row mb-4">
                    <div class="col-12 mb-4">
                        <div class="card">
                            <div class="card-body">
                                <table id="tblCourseBookingDetails" class="data-table">
                                    <thead>
                                        <tr>
                                            <th>Sno.</th>
                                            <th>OrederId</th>
                                            <th>Class Name</th>
                                            <th>Price</th>
                                            <th>Student Name</th>
                                            <th>Date</th>
                                            <th>Action</th>

                                        </tr>
                                    </thead>
                                    <tbody></tbody>
                                </table>
                            </div>
                        </div>
                    </div>
                </div>

            </div>
        </div>
    </div>
</main>


@section Scripts{
    @Html.Partial("/Views/Shared/ScriptPartials/BusinessPanel/_MyBookingScriptPartial.cshtml")
}
