<div class="container-fluid">
    <div class="row mb-4">
        <div class="col-12 mb-4">
            <div class="card mb-4">
                <div class="card-header d-flex justify-content-between align-items-center p-3">
                    <h3 class="card-title mb-4"> Category Certificate  Banner Section</h3>
                    <p class="float-lg-right">
                        <button data-toggle="collapse" href="#collapseCategoryBanner" role="button" aria-expanded="false" aria-controls="collapseExample" class="btn accordion">
                            <i class="fas fa-minus"></i>
                        </button>

                    </p>

                </div>
                <div class="collapse" id="collapseCategoryBanner">
                    <div class="card-body">
                        <form>
                            <div class="form-group">
                                <label>Icon Banner Image</label>
                                <div class="imagePreview">
                                    <img id="previewImage" class="d-none" style="width: 25%; height: 25%;">
                                </div>

                                <input type="file" id="fileProfileImage_ManageCertificateIconImage" class="border form-control" accept="image/*">
                                <div class="error-class" id="error_fileProfileImage_ManageCertificateIconImage"></div>
                            </div>
                            <div class="form-group">
                                <label for="Title">Title</label>
                                <input type="text" id="txtTitle" class="form-control" />
                                <div class="error-class" id="error_txtTitle"></div>

                            </div>
                            <div class="form-group">
                                <label for="description"> Description</label>
                                <textarea name="description" class="form-control" rows="6" id="certification_description"></textarea>
                                <div class="error-class" id="error_certification_description"></div>
                            </div>
                            <div class="form-group">
                                <label for="ButtonText">Button-Text</label>
                                <input type="text" id="txtButtonText" class="form-control" />
                                <div class="error-class" id="error_txtButtonText"></div>

                            </div>
                            <div class="form-group">
                                <label for="ButtonLink">Button-Link</label>
                                <input type="text" id="txtButtonLink" class="form-control" />
                                <div class="error-class" id="error_txtButtonLink"></div>

                            </div>
                            <div class="form-group">
                                <label for="ButtonText1">Button-Text1</label>
                                <input type="text" id="txtButtonText1" class="form-control" />
                                <div class="error-class" id="error_txtButtonText1"></div>

                            </div>
                            <div class="form-group">
                                <label for="ButtonLink1">Button-Link1</label>
                                <input type="text" id="txtButtonLink1" class="form-control" />
                                <div class="error-class" id="error_txtButtonLink1"></div>

                            </div>
                            <div class="form-group">
                                <label for="ButtonText2">Button-Text2</label>
                                <input type="text" id="txtButtonText2" class="form-control" />
                                <div class="error-class" id="error_txtButtonText2"></div>

                            </div>
                            <div class="form-group">
                                <label for="ButtonLink2">Button-Link2</label>
                                <input type="text" id="txtButtonLink2" class="form-control" />
                                <div class="error-class" id="error_txtButtonLink2"></div>

                            </div>
                            <div class="form-group">
                                <label>Certificate Banner Image</label>
                                <div class="imagePreview">
                                    <img id="previewImage" class="d-none" style="width: 25%; height: 25%;">
                                </div>

                                <input type="file" id="fileProfileImage_ManageCertificateBannerImage" class="border form-control" accept="image/*">
                                <div class="error-class" id="error_fileProfileImage_ManageCertificateBannerImage"></div>
                            </div>

                            <button type="button" class="btn btn-primary mb-0" id="ChangeUpdateText" onclick="btnSubmitAdvertisementClick()">Save</button>
                        </form>
                    </div>
                </div>

            </div>

        </div>
    </div>

