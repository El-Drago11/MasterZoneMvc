<script>
    var selectstartElement = document.getElementById('StartEducationYear');

    var currentYear = new Date().getFullYear();

    var startYear = currentYear - 50; 

    for (var i = currentYear; i > startYear ; i--) {

        var option = document.createElement('option');
        option.value = i;
        option.text = i;


        selectstartElement.appendChild(option);
    }

    var selectendElement = document.getElementById('EndEducationYear');

    for (var i = currentYear; i > startYear; i--) {

        var option = document.createElement('option');
        option.value = i;
        option.text = i;


        selectendElement.appendChild(option);
    }

    var ExperienceStartElement = document.getElementById('StartYear');

    for (var i = currentYear; i > startYear; i--) {

        var option = document.createElement('option');
        option.value = i;
        option.text = i;


        ExperienceStartElement.appendChild(option);
    }
    var ExperienceEndElement = document.getElementById('EndYear');

    for (var i = currentYear; i > startYear; i--) {

        var option = document.createElement('option');
        option.value = i;
        option.text = i;


        ExperienceEndElement.appendChild(option);
    }


   
</script>

