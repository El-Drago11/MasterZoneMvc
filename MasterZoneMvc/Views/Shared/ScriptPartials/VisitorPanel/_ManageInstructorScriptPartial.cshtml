                        truncatedAbout = `${fullAbout}`;
                    }


                    var htmlBusinessAboutfooterDetail = `<p class="dnc-cl-txt-01">${truncatedAbout} <a href="#InstructorAboutDetails">Read More</a></p>`;
                    $("#BindBusinessUser_AboutDetail").html(htmlBusinessAboutfooterDetail);
                  
                }
                if (response.data == null) {
                    $("#BindBusinessLogoDetail").addClass('d-none');
                }



                StopLoading();
            },
            error: function (result) {
                // StopLoading();

                if (result["status"] == 401) {
                    $.iaoAlert({
                        msg: 'Unauthorized! Invalid Token!',
                        type: "error",
                        mode: "dark",
                    });
                }
                else {
                    $.iaoAlert({
                        msg: 'There is some technical error, please try again!',
                        type: "error",
                        mode: "dark",
                    });
                }
            }
        });
    }


