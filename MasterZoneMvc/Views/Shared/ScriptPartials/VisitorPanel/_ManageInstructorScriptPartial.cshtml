    // Bind Business Instructor Detail
    function GetInstructorDetails() {
        $.ajax({
            type: "GET",
            url: '/api/Business/GetInstructorDetail?businessOwnerLoginId=' + BusinessOwnerLoginId_Global,
            contentType: 'application/json',
            success: function (response) {
                if (response.status < 1) {
                    $.iaoAlert({
                        msg: response.message,
                        type: "error",
                        mode: "dark",
                    });
                    return;
                }
                if (response.data.InstructorDetail === null) {
                    // Hide elements when data is null
                    $("#InstructorProfileImage, #InstructorName, #InstructorUniqueId, #Location_Section,.bio-social,#InstructorId_Section").addClass('d-none');

                } else {
                    // Update elements when data is not null
                    $("#InstructorProfileImage").attr('src', response.data.InstructorDetail.ProfileImageWithPath);
                    $("#InstructorName").html(response.data.InstructorDetail.BusinessName);
                    $("#InstructorUniqueId").html(response.data.InstructorDetail.UniqueUserId);
                    $("#InstructorFullLocation").html(response.data.InstructorDetail.Address);

                    // Continue with other updates for non-null data
                    var FacebookDetail = '';
                    if (response.data.InstructorDetail.FacebookProfileLink == '' || response.data.InstructorDetail.FacebookProfileLink === null) {
                        FacebookDetail = ``;
                    }
                    else
                    {

                        FacebookDetail = `<a target="_blank" href="${response.data.InstructorDetail.FacebookProfileLink}"><i class="penci-faicon fa fa-facebook"></i></a>`;
                        $('#InstructorFacebookProfileLink').append(FacebookDetail);
                    }

                    // Continue with Instagram and other details
                    var InstagramDetail = '';
                    if (response.data.InstructorDetail.InstagramProfileLink == '' || response.data.InstructorDetail.InstagramProfileLink == null) {
                        InstagramDetail = ``;
                    } else {
                        InstagramDetail = `<a target="_blank" href="${response.data.InstructorDetail.InstagramProfileLink}"><i class="penci-faicon fa fa-instagram"></i></a>`;
                        $('#InstructorInstagramProfileLink').append(InstagramDetail);
                    }
                    var TwitterDetail = ``;
                    if (response.data.InstructorDetail.TwitterProfileLink == '' || response.data.InstructorDetail.TwitterProfileLink == null) {
                        TwitterDetail = ``;
                    } else {
                        TwitterDetail = `<a target="_blank" href="${response.data.InstructorDetail.TwitterProfileLink}"><i class="penci-faicon fa fa-twitter"></i></a>`;
                        $('#InstructorTwitterProfileLink').append(TwitterDetail);
                    }
                    //---pending----
                    //var linkdinDetail = ``;
                    //if (response.data.InstructorDetail.LinkdinProfileLink == '' || response.data.InstructorDetail.LinkdinProfileLink == null) {
                    //    linkdinDetail = ``;
                    //} else {
                    //    linkdinDetail = `<a target="_blank" href="${response.data.InstructorDetail.LinkdinProfileLink}"><i class="penci-faicon fa fa-instagram"></i></a>`;
                    //    $('#InstructorTwitterProfileLink').append(linkdinDetail);
                    //}


                    AddOrRemoveCheckingFollowersDetail(response.data.InstructorDetail.UserLoginId);
                    var instructorLike = '';
                    instructorLike = `<a class="pl-2 pr-2">
                                        <i id="fa-icn-kjjl${response.data.InstructorDetail.UserLoginId}" onclick="AddOrRemoveFavourites(${response.data.InstructorDetail.UserLoginId});" class="heart_icon fa-regular fa-heart"></i> @*<i class="fa-solid fa-heart d-none"></i>*@
                                    </a><span class="likedstatus"></span>`;
                    $("#InstrucctorLike").append(instructorLike);
                }
                // Initialize starIcons outside of the loop
                var starIcons = '';

                if (response.data.RatingDetail.length === 0) {
                    // No ratings, set an empty string
                    starIcons = '';
                } else {
                    for (var i = 0; i < response.data.RatingDetail.length; i++) {
                        // Clear starIcons for each rating (moved outside the loop)
                        starIcons = '';

                        if (response.data.RatingDetail[i].AverageRating !== null) {
                            var rating = response.data.RatingDetail[i].AverageRating;

                            for (var j = 1; j <= 5; j++) {
                                if (j <= rating) {
                                    starIcons += '<i class="fa-solid fa-star"></i>';
                                } else {
                                    starIcons += '<i class="fa-solid fa-star" style="color: lightgrey;"></i>';
                                }
                            }
                        } else {
                            // Handle the case where AverageRating is null (or 0)
                            for (var j = 1; j <= 5; j++) {
                                starIcons += '<i class="fa-solid fa-star" style="color: lightgrey;"></i>';
                            }
                        }

                        // Append the stars for each rating
                        $('#InstructorStarDetail').append(starIcons);

                    }
                }


                // Place this outside of the loop to ensure it's only called once after all ratings are processed
                StopLoading();

            },


            error: function (result) {
                // StopLoading();

                if (result["status"] == 401) {
                    $.iaoAlert({
                        msg: 'Unauthorized! Invalid Token!',
                        type: "error",
                        mode: "dark",
                    });
                }
                else {
                    $.iaoAlert({
                        msg: 'There is some technical error, please try again!',
                        type: "error",
                        mode: "dark",
                    });
                }
            }
        });
    }
    function AddOrRemoveFavourites(id) {
      
        if (UserToken_Global == "") {
            $("#btnUserLoginPoup").click();
        }
        else {

            $.ajax({
                type: "POST",
                url: "/api/Student/AddOrRemoveStudentFavourites?favouriteUserLoginId=" + id,
                headers: {
                    "Authorization": "Bearer " + UserToken_Global,
                    "Content-Type": "application/json"
                },
                contentType: 'application/json',
                success: function (dataResponse) {
                    StopLoading();
                    //--Check if Class successfully deleted
                    if (dataResponse.status == 1)
                    {
                        //$("i#fa-icn-kjjl" + dataResponse.data.Id).addClass("fa-solid");
                        $(".heart_icon").addClass("fa-solid");
                        $(".likedstatus").html("Liked");
                    }
                    else if (dataResponse.status == 2) {
                        //$("i#fa-icn-kjjl" + dataResponse.data.Id).addClass("fa-heart");
                        $(".heart_icon").removeClass("fa-solid");
                        $(".likedstatus").html("Unliked");
                    }


                    else {
                        $.iaoAlert({
                            msg: 'There is some technical error, please try again!',
                            type: "error",
                            mode: "dark",
                        });
                    }
                },
                error: function (result) {
                    StopLoading();

                    if (result["status"] == 401) {
                        $.iaoAlert({
                            msg: 'Unauthorized! Invalid Token!',
                            type: "error",
                            mode: "dark",
                        });
                    }
                    else {
                        $.iaoAlert({
                            msg: 'There is some technical error, please try again!',
                            type: "error",
                            mode: "dark",
                        });
                    }
                }
            });
        }
    }

    function AddOrRemoveCheckingFollowersDetail(id) {



