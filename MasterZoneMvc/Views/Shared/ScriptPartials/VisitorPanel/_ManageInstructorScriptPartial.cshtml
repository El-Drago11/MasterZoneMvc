                    $("#BusinessLicensedTeachDetail_Section").addClass('d-none');
                } else {
                    for (var i = 0; i < response.data.length; i++) {

                        if (response.data[i].LicenseToTeach_Type === "Custom") {
                            BusinessLicenseToTeachDetailList = `<p class="for-licencesc-txt">${response.data[i].LicenseToTeach_DisplayName},</p>`;
                        }
                        else if (response.data[i].LicenseToTeach_Type === "Brand") {
                            BusinessLicenseToTeachDetailList = ` <p class="for-licencesc-txt">${response.data[i].Name},</p>`;
                        }
                        else if (response.data[i].LicenseToTeach_Type === "License") {
                            BusinessLicenseToTeachDetailList = ` <p class="for-licencesc-txt">${response.data[i].Title},</p>`;
                        }
                        // Append the Business Detail item to the container
                        $("#BindBusinessLicensedToTeachDetail").append(BusinessLicenseToTeachDetailList);


                    }
                }



                //StopLoading();
            },
            error: function (result) {
                //StopLoading();

                if (result["status"] == 401) {
                    $.iaoAlert({
                        msg: 'Unauthorized! Invalid Token!',
                        type: "error",
                        mode: "dark",
                    });
                }
                else {
                    $.iaoAlert({
                        msg: 'There is some technical error, please try again!',
                        type: "error",
                        mode: "dark",
                    });
                }
            }
        });
    }






</script>

