    //Bind Business Logo Detail
    function GetBusinessLogoDetail() {
        $("#BindBusinessLogoDetail").attr("src", "");
        $("#login_userName").html("");
        $("#BindBusinessUser_AboutDetail").html("");

        $.ajax({
            type: "GET",
            url: '/api/Business/GetBusinessContentAboutDetail?businessOwnerLoginId=' + BusinessOwnerLoginId_Global,
            contentType: 'application/json',
            success: function (response) {
                if (response.data != null) {
                    $("#BindBusinessLogoDetail").attr("src", response.data.BusinessLogoWithPath);
                    $("#login_userName").html(response.data.BusinessName);
                    var fullAbout = response.data.About;
                    var maxLength = 180;
                    if (fullAbout.length > maxLength) {
