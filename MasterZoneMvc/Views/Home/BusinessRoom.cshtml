<body class="rooms-template">
    
    <input type="hidden" id="hiddenBusinessOwnerLoginId" value="@ViewBag.BusinessOwnerLoginId" />
    <!-- header -->
    <header class="header">
        <div class="page-center">
            <div class="header-main">
                <div class="logo" id="BindBusinessLogoDetail">
                    @*<a href="index.html">
                        <img src="image/LOGO.png" alt="logo" loading="lazy">
                    </a>*@
                </div>
                <div class="nav-sec">
                    <div class="menu">
                        <ul>
                            <li><a href="/Home/MasterProfileAPI?businessOwnerLoginId=@ViewBag.BusinessOwnerLoginId">Home</a></li>
                            <li><a href="/Home/MasterInstructors?businessOwnerLoginId=@ViewBag.BusinessOwnerLoginId">Instructor </a></li>
                            <li><a href="/Home/MemberShip?businessOwnerLoginId=@ViewBag.BusinessOwnerLoginId">Membership</a></li>
                            <li><a href="/Home/TimeTable?businessOwnerLoginId=@ViewBag.BusinessOwnerLoginId">Time Table</a></li>
                            <li><a class="active_menu" href="/Home/BusinessRoom?businessOwnerLoginId=@ViewBag.BusinessOwnerLoginId">Room</a></li>
                            <li><a href="/Home/MasterProfileAPI?businessOwnerLoginId=@ViewBag.BusinessOwnerLoginId#contactsection">Contact Us</a></li>
                        </ul>
                    </div>
                    <div class="mobile_menu navmain-opener"><span></span></div>
                </div>
            </div>
        </div>
    </header>
    <!-- end header -->
    <div class="rooms-section">
        <div class="page-center">
            <div class="rooms-col" id="roomdetails">

            </div>
        </div>

    </div>

    <div class="table-section">
        <div class="page-center">
            <div class="table-column">
                <div class="wpb_wrapper">
                    <h3 style="text-align: center;">Pricing Table</h3>
                    
                    <table class="table table-striped">
                        <thead>
                            <tr>
                                <th>Studio</th>
                                <th style="text-align: center;">Price</th>
                                <th style="text-align: center;">Read More</th>
                            </tr>
                        </thead>
                        <tbody id="pricingtableDetails">
                        </tbody>
                    </table>


                </div>
                <div class="table-right" id="roomdetailsByPricetable">

                </div>

            </div>
        </div>
    </div>
    <button class="d-none" id="btnUserLoginPoup" data-toggle="modal" data-target="#PageOptionPopup"></button>
    <div class="modal fade" data-backdrop="static" id="PageOptionPopup" aria-modal="true" role="dialog" style="padding-right: 15px;">
        <div class="modal-dialog">
            <div class="modal-content">
                <!-- Modal Header -->
                <div class="modal-header">
                    <h2>Fill the Form</h2>
                    <button type="button" class="close" data-dismiss="modal">×</button>
                </div>
                <div class="modal-body">
                    <form>

                        <div class="row">
                            <div class="form-group col-12 d-flex">
                                <label class="control-label">Room Title:</label>
                                <div id="roomname"></div>
                                <div class="room-price" id="roomPrice"></div>
                            </div>

                        </div>
                        <div class="row">
                            <div class="form-group col-6">
                                <label class="control-label">Name<span>*</span></label>
                                <input type="text" class="form-control">
                            </div>
                            <div class="form-group col-6">
                                <label class="control-label">Surname<span>*</span></label>
                                <input type="text" class="form-control">
                            </div>
                        </div>
                        <div class="row">
                            <div class="form-group col-6">
                                <label class="control-label">Email<span>*</span></label>
                                <input type="email" class="form-control">
                            </div>
                            <div class="form-group col-6">
                                <label class="control-label">Phone<span>*</span></label>
                                <input type="number" class="form-control">
                            </div>
                        </div>

                        <div class="form-inr_full_width">
                            <div class="form-group">
                                <label class="control-label">Message</label>
                                <textarea class="form-control"></textarea>
                            </div>
                        </div>
                        <div class="submitFormbutton">
                            <button type="submit" value="Checkout" class="Submitbtn" disabled="">Submit</button>
                        </div>

                    </form>
                </div>
            </div>
        </div>
    </div>

    <div class="gallery-section">
        <div class="page-center">
            <div class="img-flex" id="Imagedetails">

            </div>
        </div>
    </div>



    <!-- footer -->
    <footer class="footer">
        <div class="page-center">
            <div class="row-sec">
                <div class="col-lg-one">
                    <div id="BindBusinessAboutDetail"></div>
                    
                    <div class="footer-social" id="BusinessSocialDetail_Footer">
