                .col-sm-9 .header .right-sec {
                    display: flex;
                    align-items: center;
                    justify-content: flex-end;
                }

                .col-sm-9 .header .logo-img img {
                    max-height: 22px;
                    display: block;
                    object-fit: contain;
                }

                .license-section .license-iner-lt .download-cta {
                    display: flex;
                    align-items: center;
                }

                .license-section .license-iner-lt .download-cta {
                    display: flex;
                    align-items: center;
                }

                .license-section .expand-rt .expand-text > * {
                    color: #fff;
                }

                .license-section .cta-link {
                    display: flex;
                    align-items: center;
                    justify-content: center;
                    padding: 8px 30px;
                    color: #000;
                    position: relative;
                    background: #fff;
                    box-shadow: 8px 8px 10px 0 rgba(0,0,0,30%);
                    border-radius: 5px;
                    border: 1px solid transparent;
                }

                .license-section .license-main-div {
                    background: #f3a762;
                    box-shadow: 8px 8px 10px 0 rgba(0,0,0,30%);
                    margin-bottom: 40px;
                    padding: 30px;
                }

                    .license-section .license-main-div:last-child {
                        margin-bottom: 0;
                    }

                .license-section .list > ul > li:before {
                    position: absolute;
                    top: 0px;
                    left: 0;
                    font-family: "Font Awesome 5 Free";
                    font-weight: 900;
                    content: "\f00c";
                    height: 20px;
                    width: 20px;
                    display: flex;
                    align-items: center;
                    justify-content: center;
                    border-radius: 50%;
                    font-size: 15px;
                    color: #fff;
                }

                .license-section .license-iner {
                    display: flex;
                    flex-wrap: wrap;
                }

                .license-section .right-iner {
                    display: flex;
                    flex-direction: column;
                    align-items: center;
                    justify-content: center;
                }

                .license-section .cta {
                    flex: 1;
                }

                .license-section .license-iner-lt .download-cta .cta-link:last-child {
                    margin-right: 0;
                }

                .license-section .license-iner .license-img > img {
                    max-height: 50px;
                    object-fit: contain;
                    border: 1px solid #fff;
                }

                .license-section .license-iner .license-img {
                    flex: 0 0 auto;
                }

                .license-section .license-iner .bolly-content {
                    flex: 1;
                    padding-left: 30px;
                }

