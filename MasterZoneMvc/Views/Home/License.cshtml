                    display: flex;
                    flex-wrap: wrap;
                    margin: 0;
                    padding: 0;
                    list-style: none;
                }

                    .license-section .list > ul > li {
                        flex: 0 0 calc(100%/2);
                        line-height: 1;
                        font-size: 16px;
                        position: relative;
                        margin-bottom: 10px;
                        padding-left: 30px;
                        font-weight: 600;
                        padding-right: 10px;
                        color: #ffff;
                    }

                .license-section .expand-div {
                    display: none;
                }

        .license-section .expand-flex {
            display: flex;
            flex-wrap: nowrap;
            margin-top: 40px;
            justify-content: space-evenly;
        }

