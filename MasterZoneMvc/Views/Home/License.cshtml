                    border-radius: 100px;
                    object-fit: contain;
                }

                .license-section .license-rt-div {
                    max-width: 400px;
                    margin: 0 auto 30px;
                    border-bottom: 6px solid #f3a762;
                    box-shadow: 8px 8px 10px 0 rgba(0,0,0,30%);
                    background: #fff;
                }

                .license-section {
                    background-color: #fff;
                    padding: 50px 0;
                }

                .license-iner .bolly-content ul > li:last-child {
                    margin-bottom: 0;
                }

                .license-section .license-main {
                    display: flex;
                    flex-wrap: wrap;
                }

                    .license-section .license-main .license-iner-lt {
                        flex: 0 0 60%;
                        max-width: 60%;
                        padding-right: 50px;
                    }

                    .license-section .license-main .license-iner-rt {
                        flex: 0 0 40%;
                        max-width: 40%;
                    }

                .license-iner .bolly-content ul {
                    padding: 0;
                    margin: 0;
                    list-style: none;
                }

                    .license-iner .bolly-content ul > li {
                        font-size: 16px;
                        font-weight: 500;
                        color: #fff;
                        margin-bottom: 5px;
                    }

                .license-iner .bolly-content > * {
                    color: #fff;
                }


                .license-section .arrow-down {
                    display: flex;
                    justify-content: center;
                }

                .license-section .cta-link:hover {
                    border: 1px solid #fff;
                    background: #f3a762;
                    color: #fff;
                }

                .license-section .arrow-down > i {
                    font-size: 30px;
                    color: #fff;
                    cursor: pointer;
                }

                .license-section .license-heading > * {
                    margin-bottom: 0;
                    color: #fff;
                    text-align: center;
                }

                .license-section .license-heading {
                    padding: 10px;
                    background: #f3a762;
                }

                .license-section .logo-iner {
                    padding: 20px;
                    display: flex;
                    flex-wrap: wrap;
                }

                .license-section .license-text {
                    flex: 1;
                    padding-left: 20px;
                }

                    .license-section .license-text > * {
                        margin-bottom: 0;
                    }

                        .license-section .license-text > *:first-child {
                            margin-bottom: 8px;
                        }

                    .license-section .license-text p {
                        font-size: 15px;
                    }

                @@media(max-width:991px) {
                    .license-section .license-main .license-iner-lt {
                        flex: 0 0 100%;
                        padding-right: 0;
                        max-width: 100%;
                        margin: 0 auto;
                        order: 1;
                    }
                    .license-section .expand-flex {                     
                        flex-wrap: wrap;                       
                    }

                    .license-section .license-main .license-iner-rt {
                        flex: 0 0 100%;
                        max-width: 800px;
                        margin: 0 auto;
                        order: 0;
                    }

                    .license-section .license-rt-div {
                        max-width: 100%;
                    }

                    .license-section .expand-lt {
                        flex: 0 0 100%;
                        max-width: 100%;
                    }

                    .license-section .expand-rt {
                        flex: 0 0 100%;
                        max-width: 100%;
                        border-left: 0;
                        border-top: 1px solid #fff;
                        padding-top: 20px;
                        margin-top: 20px;
                    }

                    .license-section {
                        padding: 50px 0;
                    }
                }


                @@media(max-width:580px) {
                    header .header-iner .logo-img {
                        text-align: center;
                    }
                    .license-section .license-iner .license-img > img {
                        max-height: 100%;
                    }

                    .header .header-iner > div {
                        flex: 0 0 100%;
                        max-width: 100%;
                        justify-content: center;
                    }

                    .license-section .right-iner {
                        flex: 0 0 100%;
                        max-width: 100%;
                        flex-direction: inherit;
                        margin-top: 20px;
                    }

                    .license-section .cta {
                        padding-right: 20px;
                    }

                    .license-section .license-iner .license-img {
                        flex: 0 0 100%;
                        max-width: 100%;
                    }

                    .license-section .license-iner .bolly-content {
                        padding-left: 0;
                        margin-top: 10px;
                    }

                    .license-section .arrow-down {
                        flex: 0 0 auto;
                        justify-content: flex-end;
                    }

                    .license-section .license-main-div {
                        padding: 25px;
                    }
                }
    </style>
}
