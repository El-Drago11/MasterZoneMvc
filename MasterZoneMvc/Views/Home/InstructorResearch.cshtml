<div class="new-templates">
    <div class="form-section" style="background-image: url('/Content/visitor-panel/image/dark-pattern.png');">

        <div class="page-center">

            <div class="form-iner">
                <form action="">
                    <div class="form-items filter-form-items">
                        <div class="half-width filter-type-location">
                            <label for="txtFilterTypeLocation">Location</label>
                            <input type="search" id="txtFilterTypeLocation" placeholder="Choose your location" autocomplete="off">
                        </div>
                        <div class="half-width filter-type-name">
                            <label for="txtFilterTypeName">Name</label>
                            <input type="text" id="txtFilterTypeName" placeholder="Name" autocomplete="off">
                        </div>

                        <div class="half-width filter-type-category">
                            <label for="ddlFilterTypeCategory">Category</label>
                            <select class="" id="ddlFilterTypeCategory">
                                <option value="0" selected="selected">Select Category</option>
                            </select>
                            <div class="error-class" id="error_ddlFilterTypeCategory"></div>
                        </div>

                        @*<div class="half-width Types-iner filter-type-category">
                                <select class="Types-iners" id="ddlProfilePageTypeId_InstructorRearch">
