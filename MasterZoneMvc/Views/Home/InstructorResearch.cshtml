<button class="d-none" id="btnUserLoginPoup" data-toggle="modal" data-target="#LoginPageOptionPopup"></button>
<div class="modal fade" data-backdrop="static" id="LoginPageOptionPopup" style="padding: 196px 1px 169px 347px;">
    <div class="modal-dialog modal-lg">
        <div class="modal-content" style="width: 50%; align-items: center;">
            <!-- Modal Header -->
            <div class="modal-header">
                <h4 class="modal-title">Please Login</h4>
                <button type="button" class="close" data-dismiss="modal">×</button>
            </div>
            <!-- Modal footer -->
            <div class="modal-footer">
                <button id="submitEnquiry" type="button" class="btn btn-outline-dark cstm-btn w-auto">
                    <a href="/Home/Login">
                        Login
                    </a>
                </button>

            </div>
        </div>
    </div>
</div>

