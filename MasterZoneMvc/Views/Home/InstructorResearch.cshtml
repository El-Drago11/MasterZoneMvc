                                <div class="error-class" id="error_ddlProfilePageTypeId_InstructorRearch"></div>
                            </div>

                            <div class="half-width class-iners filter-type-class-category">
                                <select class="class-iner" id="CategoryDetailDropdown">
                                    <option value="0" selected="selected">Class Category</option>
                                </select>
                                <div class="error-class"></div>
                            </div>

                            <div class="half-width Instructor-iners filter-type-instructor-category">
                                <select class="Instructor-iner" id="InstructorListDropdown">
                                    <option value="0" selected="selected">Instructor Category</option>
                                </select>
                                <div class="error-class"></div>
                            </div>*@

                        <div class="half-width filter-type-mode">
                            <label for="ddlFilterTypeMode">Mode</label>
                            <select class="" id="ddlFilterTypeMode">
                                <option value="" selected="selected">Select Mode</option>
                                <option value="online">Online</option>
                                <option value="offline">Offline</option>
                            </select>
                        </div>

                        <div class="half-width filter-type-price">
                            <label for="ddlFilterTypePrice">Price</label>
                            <select class="" id="ddlFilterTypePrice">
                                <option value="" selected="selected">Select Price</option>
                                <option value="free">Free</option>
                                <option value="paid">Paid</option>
                            </select>
                            <div class="error-class"></div>
                        </div>

                        <div class="half-width filter-type-startdate">
                            <label for="txtFilterStartDate">Start Date</label>
                            <input type="date" id="txtFilterStartDate" />
                            <div class="error-class"></div>
                        </div>

                        <div class="half-width filter-type-days">
                            <label for="ddlFilterTypeDays">Days</label>
                            <select id="ddlFilterTypeDays">
                                <option value="">Select Day</option>
                                <option value="Sunday">Sunday</option>
                                <option value="Monday">Monday</option>
                                <option value="Tuesday">Tuesday</option>
                                <option value="Wednesday">Wednesday</option>
                                <option value="Thursday">Thursday</option>
                                <option value="Friday">Friday</option>
                                <option value="Saturday">Saturday</option>
                            </select>
                            <div class="error-class"></div>
                        </div>

                        <div class="submit-cta">
                            <button type="button" class="primary-button mt-4" style="background: #f3a762;" onclick="onClickSearch();">Search</button>
                        </div>
                    </div>


                </form>
            </div>
            <div class="radio-selection-section">
                <ul class="input-radio-option">
                    <li>
                        <label class="radio-inline">
                            <input type="radio" id="AcademiesId" data-name="academies" name="filter-item-type">Academies
