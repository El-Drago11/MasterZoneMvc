                return true;
            } else {
                $(err_id).html('Please enter a alpha-numeric number and allow only underscores.');
                return false;
            }
        }

        function validateDateOfBirthFormat(inputElementId) {
            var err_id = '#error_' + inputElementId;
            var value = $('#' + inputElementId).val();

            if (value == "") {
                $(err_id).html('');
                return false;
            }

            var parts = value.split('/');
            var day = parseInt(parts[0], 10);
            var month = parseInt(parts[1], 10);
            var year = parseInt(parts[2], 10);

            if (parts.length === 3 && day >= 1 && day <= 31 && month >= 1 && month <= 12 && year >= 1900 && year <= 9999) {
                $(err_id).html('');
                return true;
            } else {
                $(err_id).html('Please enter a valid date in the format DD/MM/YYYY. example:- 15/05/1999 ');
                return false;
            }
        }

        function GetCurrentDate_DD_MM_YYYY() {
            var currentDate = new Date();

            var day = currentDate.getDate();
            var month = currentDate.getMonth() + 1; // Months are 0-based
            var year = currentDate.getFullYear();

            // Add leading zeros if day or month is a single digit
            day = day < 10 ? '0' + day : day;
            month = month < 10 ? '0' + month : month;

            var formattedDate = day + '/' + month + '/' + year;
            return formattedDate;
        }

