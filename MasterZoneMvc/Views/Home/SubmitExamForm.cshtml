@using MasterZoneMvc.ViewModels;
@model ExamFormResponse_VM



@{
    Layout = "~/Views/Shared/_VisitorLayout.cshtml";
    ViewData["Title"] = "Masterzone";
    long itemcourseid = ViewBag.Itemcourseid;
}
@{
    string[] titleParts = Model.NameWithAddress.Split(new[] { "\r\n" }, StringSplitOptions.None);
}

@section Styles {

    <style>
        .image {
            width: 75px;
            height: 75px;
        }

        .imagePreview {
            border: 2px dashed #cecece;
            height: 75px;
            width: 75px;
            margin: 5px 0px;
            padding: 5px;
        }

        p {
            margin-bottom: 4px;
            font-weight: bold;
        }

        #section-1 input {
            margin-bottom: 10px;
            border: 1px solid #f3a762;
        }

        .Orange_White_Colour {
            background: #f3a762;
            color: #fff;
        }

        .profile-image-section {
            width: 224px;
            height: 254px;
            border: 2px solid #f3a762;
            margin-top: 38px;
            display: flex;
            justify-content: center;
        }

        img#profileImage {
            width: 100%
        }

        .signature-image-section {
            width: 260px;
            height: 110px;
            border: 2px solid #f3a762;
            display: flex;
            justify-content: center;
        }

        #category input {
            height: unset;
            width: unset;
        }

        img#signature {
            width: 100%
        }

        ol li {
            list-style: unset;
        }

        .box-dot {
            /*border: 0;
            border-bottom: 1px dashed #f3a762;
            height: 20px;
            margin-bottom: 10px;
            width: 100%;
            outline: 0;*/
        }

        .form-control:focus {
            box-shadow: unset;
        }

        ol, ul {
            margin: inherit;
            padding: inherit !important;
        }
    </style>
}



<div id="exam-page">


    <section id="section-1">
        <div class="container">
            @*ESTD, Logo, Business Name ,address, contact Details*@
            <div class="exam-page-row-1 row mb-4">
                @*ESTD*@
                <div class="col-sm-2">
                    <p>ESTD. @Model.EstablishedYear</p>
                    <img src="@Model.ExamFormLogoWithPath" class="ls-p-lg mblg-ls  mr-2" style="width: 86px;">
                </div>

                @*Session and Business Name address*@
                <div class="col-sm-6 text-center">
                    <h4 class="mb-2">Valid for @Model.Title session</h4>
                    @for (int n = 1; n <= titleParts.Length; n++)
                    {
                        <h@(n+1)>@Html.Raw(HttpUtility.HtmlDecode(titleParts[n - 1]))</h@(n+1)>
                    }
                </div>

                @*Contact Details*@
                <div class="col-sm-4 text-right">
                    <p class="m-0">New Telephone Nos.</p>
                    <p class="m-0">
                        Secretary Chamber: <span id="exampage-top-Secretary-number">@Model.SecretaryNumber</span>
                    </p>
                    <p class="m-0">
                        Registrar Office: <span id="exampage-top-Registrar-number">@Model.RegistrarOfficeNumber</span>
                    </p>
                    <p class="m-0">Website: <span id="exampage-top-website">@Model.WebsiteLink</span></p>
                    <p class="m-0">E-mail: <span id="exampage-top-email">@Model.Email</span></p>
                </div>
            </div>

            @*Profile Image and Important Instruction*@
            <div class="exam-page-row-2 row">
                @*Profile Image*@
                <div class="col-sm-4">
                    <div class="profile-image-section">
                        <img src="" id="profileImg">
                    </div>
                    <input id="CandidateProfileImage" class="form-control" type="file" style="border: none;" onchange="validateImage(this.id)">
                    <div class="error-class" id="error_CandidateProfileImage"></div>
                </div>

                @*Important Instruction*@
                <div class="col-sm-8">
                    <h5 class="text-center">IMPORTANT INSTRUCTION</h5>
                    <ol>
                        @{
                            List<string> instructions = null;
                            if (Model.ImportantInstruction != null)
                            {
                                instructions = Model.ImportantInstruction
.Split(new[] { "<li>", "<ol>", "</li>", "</ol>" }, StringSplitOptions.RemoveEmptyEntries)
.Select(instruction => instruction.Replace("</li>", "").Replace("</ol>", "").Replace("<ol>", "").Replace("<li>", ""))
.ToList();
                            }
                            foreach (var instruction in instructions)
                            {
                                <li>@Html.Raw(HttpUtility.HtmlDecode(instruction))</li>
                            }
                        }
                    </ol>
                </div>
            </div>

            @*Exam Form Details*@
            <div class="exam-page-row-3 row">
                <div class="col-sm-12 m-0">

                    @*Note and session year*@
                    <div class="row mb-2">
                        @*note*@
                        <div class="col-sm-12">
                            <h5 class="text-center mt-3 text-danger">*Note : Form will not be accepted without Image.</h5>
                        </div>
                        @*Session Year*@
                        <div class="col-sm-12">
                            <h4 class="d-flex">
                                Examination Application Form for @Model.Title<input id="SessionYear" type="text" class="form-control box-dot w-auto pt-3"> Examination
                            </h4>
                        </div>
                    </div>
                    x
                    @*Category and Roll NO*@
                    <div class="row">
                        @*Category*@
                        <div class="col-sm-6  d-flex align-items-center" id="category">
                            <p class="pr-3">Category<span class="text-danger">*</span></p>
                            <label class="pr-sm-3" for="General">
                                <input type="radio" name="category" id="General"
                                       value="General" onchange="validateRadioButton(this.name)">
                                General
                            </label>

                            <label class="pr-sm-3" for="Teacher">
                                <input type="radio" name="category" id="Teacher"
                                       value="Teacher" onchange="validateRadioButton(this.name)">
                                Teacher
                            </label>

                            <label class="pr-sm-3" for="ImprovementDiv">
                                <input type="radio" name="category" id="ImprovementDiv" value="Improvement for Division" onchange="validateRadioButton(this.name)">
                                Improvement for Division.
                            </label>
                            <div class="error-class" id="error_category"></div>
                        </div>
                        @*Roll No*@
                        <div class="col-sm-6">
                            <p class="ml-sm-1">Candidate Roll No.<span class="text-danger">*</span></p>
                            <input id="CurrentRollNo" class="form-control box-dot" type="text" disabled>
                        </div>
                    </div>

                    @*MasteId,BusinessId, CenterNo*@
                    <div class="row">
                        <div class="col-sm-4 ">
                            <p>Master-Id<span class="text-danger">*</span></p>
                            <input id="UserMasterId" class="form-control box-dot" type="text" oninput="validateMasterId(this.id)">
                            <div class="error-class" id="error_UserMasterId"></div>
                        </div>
                        <div class="col-sm-4">
                            <p>Business-Id<span class="text-danger">*</span></p>
                            <input id="BusinessId" class="form-control box-dot" type="text" value="@Model.BusinessMasterId" disabled>
                        </div>
                        <div class="col-sm-4">
                            <p>Center-No<span class="text-danger">*</span></p>
                            <input id="CenterNo" class="form-control box-dot" type="text" value="@Model.CenterNo" disabled>
                        </div>
                    </div>

                    @*ALl input list*@
                    <div class="row">
                        <ol class="px-5">
                            @*Candidtae Name*@
                            <li class="mt-1">
                                <p> Name of the Candidate in English(Block Letters)<span class="text-danger">*</span></p>
                                <input id="CandidateName" type="text" class="form-control box-dot" oninput="validateAlphabetsAndSpaces(this.id)">
                                <div class="error-class" id="error_CandidateName"></div>
                            </li>

                            @*Father Name*@
                            <li>
                                <p> Father's Name in English (Block Letters)<span class="text-danger">*</span></p>
                                <input id="CandidateFather" type="text" class="form-control box-dot" oninput="validateAlphabetsAndSpaces(this.id)">
                                <div class="error-class" id="error_CandidateFather"></div>
                            </li>

                            @*Mother Name*@
                            <li class="mt-1">
                                <p>Mother's Name in English (Block Letters)<span class="text-danger">*</span></p>
                                <input id="CandidateMother" type="text" class="form-control box-dot" oninput="validateAlphabetsAndSpaces(this.id)">
                                <div class="error-class" id="error_CandidateMother"></div>
                            </li>

                            @*Permanent Address*@
                            <li class="mt-1">
                                <p>Permanent Address (Block Letters)<span class="text-danger">*</span></p>
                                <input id="PermanentAddress" type="text" class="form-control box-dot">
                                <div class="error-class" id="error_PermanentAddress"></div>
                                <div class="row">
                                    <div class="col-sm-6">
                                        <p>Pin Code<span class="text-danger">*</span></p>
                                        <input id="PermanentPin" type="text" class="form-control box-dot" oninput="validateNonNegativeNonDecimalNonEmptyAnd6Digit(this.id)">
                                        <div class="error-class" id="error_PermanentPin"></div>
                                    </div>
                                    <div class="col-sm-6">
                                        <p>Phone/Mob. No<span class="text-danger">*</span></p>
                                        <input id="PermanentMobNo" type="text" class="form-control box-dot" oninput="validatePhoneNumber(this.id)">
                                        <div class="error-class" id="error_PermanentMobNo"></div>
                                    </div>
                                </div>
                            </li>

                            @*Present Address*@
                            <li class="mt-1">
                                <p>Present Address (Block Letters)<span class="text-danger">*</span></p>
                                <input id="PresentAddress" type="text" class="form-control box-dot">
                                <div class="error-class" id="error_PresentAddress"></div>
                                <div class="row">
                                    <div class="col-sm-6">
                                        <p>Pin Code<span class="text-danger">*</span></p>
                                        <input id="PresentPin" type="text" class="form-control box-dot" oninput="validateNonNegativeNonDecimalNonEmptyAnd6Digit(this.id)">
                                        <div class="error-class" id="error_PresentPin"></div>
                                    </div>
                                    <div class="col-sm-6">
                                        <p>Phone/Mob. No<span class="text-danger">*</span></p>
                                        <input id="PresentMobNo" type="text" class="form-control box-dot" oninput="validatePhoneNumber(this.id)">
                                        <div class="error-class" id="error_PresentMobNo"></div>
                                    </div>
                                </div>
                            </li>

                            @*Nationality and AadharCard No*@
                            <li>
                                <div class="row">
                                    <div class="col-sm-6">
                                        <p>(I.) Nationality<span class="text-danger">*</span></p>
                                        <input id="Nationality" class="form-control box-dot" type="text" oninput="validateAlphabetsAndSpaces(this.id)">
                                        <div class="error-class" id="error_Nationality"></div>
                                    </div>
                                    <div class="col-sm-6">
                                        <p>(II.) AadharCard No<span class="text-danger">*</span></p>
                                        <input id="AadharCardNo" class="form-control box-dot" type="text" oninput="validateNonNegativeNonDecimalNonEmptyAnd12Digit(this.id)">
                                        <div class="error-class" id="error_AadharCardNo"></div>
                                    </div>
                                </div>
                            </li>

                            @*DOB and Email*@
                            <li>
                                <div class="row">
                                    <div class="col-sm-6">
                                        <p>(I.) DOB (DD/MM/YYYY)<span class="text-danger">*</span></p>
                                        <input id="DOB" class="form-control box-dot" type="text" oninput="validateDateOfBirthFormat(this.id)">
                                        <div class="error-class" id="error_DOB"></div>
                                    </div>
                                    <div class="col-sm-6">
                                        <p>(II.) Email<span class="text-danger">*</span></p>
                                        <input id="Email" class="form-control box-dot" type="text" oninput="validateEmail(this.id)">
                                        <div class="error-class" id="error_Email"></div>
                                    </div>
                                </div>
                            </li>

                            @*Education qualification*@
                            <li>
                                <p>Educational qualification<span class="text-danger">*</span></p>
                                <input id="EduQualification" class="form-control box-dot" type="text" oninput="validateAlphabetsNumberAndSpaces(this.id)">
                                <div class="error-class" id="error_EduQualification"></div>
                            </li>

                            @*Examination in which the candidate wants to appear*@
                            <li>
                                <p>Details of Examination in which the candidate wants to appear-</p>
                                <div class="row">
                                    <div class="col-sm-6">
                                        <p>(I.) Class<span class="text-danger">*</span></p>
                                        <input id="CurrentClass" class="form-control box-dot" type="text" oninput="validateAlphabetsNumberAndSpaces(this.id)">
                                        <div class="error-class" id="error_CurrentClass"></div>
                                    </div>
                                    <div class="col-sm-6">
                                        <p>(II.) Subject<span class="text-danger">*</span></p>
                                        <input id="CurrentSubject" class="form-control box-dot" type="text" oninput="validateAlphabetsAndSpaces(this.id)">
                                        <div class="error-class" id="error_CurrentSubject"></div>
                                    </div>
                                </div>
                                <div class="row">
                                    <div class="col-sm-6">
                                        <p>(III.) Center Name<span class="text-danger">*</span></p>
                                        <input id="CurrentCenterName" class="form-control box-dot" type="text" oninput="validateAlphabetsNumberAndSpaces(this.id)">
                                        <div class="error-class" id="error_CurrentCenterName"></div>
                                    </div>
                                    <div class="col-sm-6">
                                        <p>(IV.) City<span class="text-danger">*</span></p>
                                        <input id="CurrentCenterCity" class="form-control box-dot" type="text" oninput="validateAlphabetsAndSpaces(this.id)">
                                        <div class="error-class" id="error_CurrentCenterCity"></div>
                                    </div>
                                </div>
                            </li>

                            @*Previous examination Details*@
                            <li>
                                <p>
                                    For eligibility to appear in the examination aforementioned, attach X-rox
                                    copies of previous examination marksheer/certificates duly attested.
                                    Details of last music examination passed.
                                </p>
                                <div class="row">
                                    <div class="col-sm-4">
                                        <p>(I.) Class<span class="text-danger">*</span></p>
                                        <input id="PreviousClass" class="form-control box-dot" type="text" oninput="validateAlphabetsNumberAndSpaces(this.id)">
                                        <div class="error-class" id="error_PreviousClass"></div>
                                    </div>
                                    <div class="col-sm-4">
                                        <p>(II.) Subject<span class="text-danger">*</span></p>
                                        <input id="PreviousSubject" class="form-control box-dot" type="text" oninput="validateAlphabetsAndSpaces(this.id)">
                                        <div class="error-class" id="error_PreviousSubject"></div>
                                    </div>
                                    <div class="col-sm-4">
                                        <p>(III.) Year<span class="text-danger">*</span></p>
                                        <input id="PreviousYear" class="form-control box-dot" type="text" oninput="validateYearFrom2000(this.id)">
                                        <div class="error-class" id="error_PreviousYear"></div>
                                    </div>
                                </div>
                                <div class="row">
                                    <div class="col-sm-6">
                                        <p>(IV.) Roll No<span class="text-danger">*</span></p>
                                        <input id="PreviousRollNo" class="form-control box-dot" type="text" oninput="validateNonNegativeNonDecimalNonEmpty(this.id)">
                                        <div class="error-class" id="error_PreviousRollNo"></div>
                                    </div>
                                    <div class="col-sm-6">
                                        <p>(V.) Result<span class="text-danger">*</span></p>
                                        <input id="PreviousResult" class="form-control box-dot" type="text">
                                        <div class="error-class" id="error_PreviousResult"></div>
                                    </div>
                                </div>
                                <div class="row">
                                    <div class="col-sm-12">
                                        <p>(VI.) Center<span class="text-danger">*</span></p>
                                        <input id="PreviousCenterName" class="form-control box-dot" type="text" oninput="validateAlphabetsNumberAndSpaces(this.id)">
                                        <div class="error-class" id="error_PreviousCenterName"></div>
                                    </div>
                                </div>
                            </li>

                            @*Amount of Fee*@
                            <li>
                                <p>Amount of fee paid to the Centre Superintendent.</p>
                                <div class="row">
                                    <div class="col-sm-5">
                                        <p>Amount<span class="text-danger">*</span></p>
                                        <input id="Amount" class="form-control box-dot" type="text" oninput="validateNonNegativeNonDecimalNonEmpty(this.id)">
                                        <div class="error-class" id="error_Amount"></div>
                                    </div>
                                    <div class="col-sm-7">
                                        <p>(In Word.)<span class="text-danger">*</span></p>
                                        <input id="AmountInWord" class="form-control box-dot" type="text" oninput="validateAlphabetsAndSpaces(this.id)">
                                        <div class="error-class" id="error_AmountInWord"></div>
                                    </div>
                                </div>
                            </li>

                            @*No of Attached*@
                            <li>
                                <p>Number of enclosures attached<span class="text-danger">*</span></p>
                                <input id="NoOfAttached" class="form-control box-dot" type="text" oninput="validateNonNegativeNonDecimalNonEmpty(this.id)">
                                <div class="error-class" id="error_NoOfAttached">
                                </div>
                            </li>

                            @*Certificate required at home or superintendent *@
                            <li>
                                <p>
                                    Mark-sheet and Certificate required at home address or through Centre
                                    Superintendent
                                </p>
                                <input id="CertificateCollectFrom" class="form-control box-dot" type="text">
                                <div class="error-class" id="error_CertificateCollectFrom"></div>
                            </li>

                            @*Signature*@
                            <li>
                                <p>Signature of Candidate<span class="text-danger">*</span></p>
                                <div class="signature-image-section">
                                    <img src="" id="signatureImg">
                                </div>
                                <input id="CandidateSignature" class="form-control" type="file" style="border: none;" onchange="validateImage(this.id)">
                                <div class="error-class" id="error_CandidateSignature"></div>
                            </li>

                        </ol>

                    </div>

                    @*Term and conditions*@
                    <div class="row">
                        @*Term and condtion points*@
                        <div class="col-sm-12">
                            <ol type="i">
                                <li>I have carefully read the rules, regulations and the syllabus concerning the examination and I assure that I will abide by them in word and spirit.</li>
                                <li>I enclose with this form the prescribed examination fee by Bank Draft / Postal Order or have deposited the fee in cash with the Centre Superintendent.</li>
                                <li>I agree to appear in the examination at a place other than the centre mentioned in this form which should be Registrar so decide.</li>
                                <li>I agree to the condition that subject matter of any dispute concerning Prayag Sangit Samiti and myself will be subject to jurisdiction of courts only in Prayagraj (Allahabad).</li>
                                <li>Entries from 1 to 11 are correct to the best of my knowledge.</li>
                            </ol>
                        </div>

                        @*Current date*@
                        <div class="col-sm-6 text-left mt-5">
                            <p>Current Date<span class="text-danger">*</span></p>
                            <input id="termAndConditionsDate" class="form-control box-dot w-50" type="text" style="background-color:unset" disabled>
                        </div>

                        @*signature*@
                        <div class="col-sm-6" style="display: flex; align-items: flex-end; flex-direction: column;">
                            <div style="width: 260px; height: 90px; border: 2px solid #f3a762; display: flex; justify-content: center;">
                                <img src="" id="signatureAtTermAndConditions">
                            </div>
                            <p>Full Signature of the Candidate<span class="text-danger">*</span></p>
                        </div>
                    </div>
                    <hr />
                    @*Decleration by father / Guardian(If Candiate is minor)*@
                    <div class="row m-0 p-0 mt-4">
                        @*Title*@
                        <div class="col-sm-12 m-0 p-0 text-center">
                            <h3 class="text-danger">DECLERATION BY FATHER/GUARDIAN(IF CANDIDATE IS MINOR)</h3>
                        </div>
                        @*Declareation*@
                        <div class="col-sm-12 m-0 p-0" id="declerationByGuardian">
                            I hereby declare that I have carefully read the rules, regulations and syllabus concerning the examination and assure that my ward is fit and eligible to appear in the examination. I take full responsibility of following the prescribed rules, regulations and directions/orders of the Registrar concerning the examination by my ward. The Provision (iv) above regarding jurisdiction of Courts at Prayagraj (Allahabad) is acceptable to me.
                        </div>

                        @*Current Date*@
                        <div class="col-sm-6 m-0 p-0 text-left mt-5">
                            <p>Current Date<span class="text-danger">*</span></p>
                            <input id="declerationByGuardianDate" class="form-control box-dot w-50" type="text" style="background-color:unset" disabled>
                        </div>

                        @*Signature*@
                        <div class="col-sm-6 m-0 p-0" style="display: flex; align-items: flex-end; flex-direction: column;">
                            <div style="width: 260px; height: 90px; border: 2px solid #f3a762; display: flex; justify-content: center;">
                                <img src="" id="signatureOfGuradianImg">
                            </div>
                            <p>Signature of Father/Guradian<span class="text-danger">*</span></p>
                            <input id="CandidateGuradianSignature" class="w-50" type="file" style="border: none;" onchange="validateImage(this.id)">
                            <div class="error-class" id="error_CandidateGuradianSignature"></div>
                            <div class="d-flex mt-3">
                                <p style="margin-right:10px">Full Name<span class="text-danger">*</span></p>
                                <input id="CandidateGuradianName" class="form-control box-dot w-auto m-0 p-0" type="text" oninput="validateAlphabetsAndSpaces(this.id)">
                                <div class="error-class" id="error_CandidateGuradianName"></div>
                            </div>
                        </div>
                    </div>
                    @*Decleration by Superintendent*@
                    <!--<div class="row m-0 p-0 mt-4">-->
                    @*Title*@
                    <!--<div class="col-sm-12 m-0 p-0 text-center">
                        <h3 class="text-danger">SIGNATURE BY CENTRE SUPERINTENDENT</h3>
                    </div>-->
