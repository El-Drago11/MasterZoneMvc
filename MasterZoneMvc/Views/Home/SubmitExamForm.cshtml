                    </div>-->
                    @*Current Date*@
                    <!--<div class="col-sm-4 m-0 p-0 text-left mt-5">
                        <p>Current Date<span class="text-danger">*</span></p>
                        <input id="declerationBySuperintendentDate" class="box-dot w-50" type="text" style="background-color:unset" disabled>
                    </div>-->
                    @*Center No*@
                    <!--<div class="col-sm-4 m-0 p-0 text-left mt-5">
                        <p>Center No<span class="text-danger">*</span></p>
                        <input id="declerationBySuperintendentCenterNo" class="box-dot w-50" type="text" style="background-color:unset" value="@Model.CenterNo" disabled>
                    </div>-->
                    @*Signature*@
                    <!--<div class="col-sm-4 m-0 p-0" style="display: flex; align-items: flex-end; flex-direction: column;">
