            else if (/^\d{12}$/.test(value) && parseInt(value) >= 0 && value !== "") {
                $(err_id).html('');
                return true;
            } else {
                $(err_id).html('Please enter 12-digit non-decimal number.');
                return false;
            }
        }

        function validateYearFrom2000(inputElementId) {
            var err_id = '#error_' + inputElementId;
            var value = $('#' + inputElementId).val();
            if (value == "") {
                $(err_id).html('');
                return false;
            }
