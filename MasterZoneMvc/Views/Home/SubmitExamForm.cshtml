        function validatePhoneNumber(inputElementId) {
            var err_id = '#error_' + inputElementId;
            var value = $('#' + inputElementId).val();
            if (value == "") {
                $(err_id).html('');
                return false;
            }
            else if (/^[\+]?[(]?[0-9]{3}[)]?[-\s\.]?[0-9]{3}[-\s\.]?[0-9]{4,6}$/im.test(value)) {
                $(err_id).html('');
                return true;
            } else {
                $(err_id).html('Please enter valid Phone Number.');
                return false;
            }
        }

        function validateEmail(inputElementId) {
            var err_id = '#error_' + inputElementId;
            var value = $('#' + inputElementId).val();
            if (value == "") {
                $(err_id).html('');
                return false;
            }
            else if (/^([\w-\.]+)@@((\[[0-9]{1,3}\.[0-9]{1,3}\.[0-9]{1,3}\.)|(([\w-]+\.)+))([a-zA-Z]{2,4}|[0-9]{1,3})(\]?)$/.test(value)) {
                $(err_id).html('');
                return true;
            } else {
                $(err_id).html('Please enter valid Email Id.');
                return false;
            }
        }

        function validateImage(inputElementId) {

            var file = $('#' + inputElementId).get(0).files[0];
            var err_id = '#error_' + inputElementId;

            if (file) {
                var fileType = file.type.split('/')[0]; // Check if it's an image

                if (fileType === 'image') {
                    console.log('Valid image selected.');
                    $(err_id).html('');
                    return true;
                } else {
                    //alert('Please select a valid image file.');
                    $('#' + inputElementId).val(''); // Reset the input
                    $(err_id).html('Please select valid image (jpg,jpeg,png).');
                    return false;
                }
            } else {
                alert('Please select a file.');
                return false;
            }
        }

        function validateRadioButton(inputElementId) {
            var selected = $("input[name='" + inputElementId + "']:checked").length;

            if (selected > 0) {
                $("#error_" + inputElementId).text('');
                return true;
            } else {
                $("#error_" + inputElementId).text('Please select at least one option.');
                return false;
            }
