<style>

    .btnViewMoreUpcomingImages {
        display: none !important;
    }

    .btnViewMoreUpcomingVideos {
        display: none !important;
    }

    .btnViewMoreUpcomingDetail {
        display: none !important;
    }

    .header.header {
        background: #f3a762;
        border-bottom: 1px solid #ffffff85;
    }

    footer#footer {
        display: none;
    }

    .copyright_sec {
        color: white;
    }
    @@media(max-width: 800px) {

        #section-yoga-8 .slick-slide {
            min-width: auto;
        }

        div#contact {
            margin-top: 456px;
        }

        .contact-section .address-sec {
            padding: 20px 7px;
        }

        #BindBusinessAllImageAndVideos .img-inr {
