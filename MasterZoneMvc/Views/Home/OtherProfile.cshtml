            margin-top: 20px;
        }
        .address-text > * {
            font-size: 18px;
        }
        #BindBusinessPersonalInformation .deatil-inr {
            display: grid;
            grid-template-columns: repeat(1,1fr);
        }
    }
    #BindBusinessAllImageAndVideos .img-inr {
        margin-left: 20px;
        margin-top: 20px;
    }

    #BindBusinessImages .img-inr {
        margin-left: 20px;
        margin-top: 20px;
