                </div>
            </div>
        </div>
        <!-- about me -->
        <div class="about-me about  tab-items " id="about_tabs">
            <div class="about-me-details container">
                <div class="about-heading">
                    <div class="title-section text-start text-sm-center"><h1>ABOUT <span>ME</span></h1></div>
                </div>
                <!-- about details -->
                <div class="about-col-flex">
                    <div class="about-lt-col">
                        <div class="lt-col-in">
                            <h3 class="text-uppercase custom-title mb-0 ft-wt-600">Personal Information</h3>
                            <ul class="about-list list-unstyled open-sans-font">
                                <li><span class="title">first name: </span><span class="value d-block d-sm-inline-block d-lg-block d-xl-inline-block " id="InstructorFirstName"></span></li>
                                <li><span class="title">last name: </span><span class="value d-block d-sm-inline-block d-lg-block d-xl-inline-block " id="InstructorLastName"></span></li>
                                <li><span class="title">Age: </span><span class=" InstructorAge value d-block d-sm-inline-block d-lg-block d-xl-inline-block "></span></li>
                                <li><span class="title">Nationality: </span><span class="value d-block d-sm-inline-block d-lg-block d-xl-inline-block " id="InstructorNationality"></span></li>
                                <li><span class="title">Freelance: </span><span class="value d-block d-sm-inline-block d-lg-block d-xl-inline-block green" id="Instructorfreelance"></span></li>
                                <li><span class="title">Address: </span><span class=" InstructorAddress value d-block d-sm-inline-block d-lg-block d-xl-inline-block  "></span></li>
                                <li><span class="title">Country Code: </span><span class="InstructorNumberCountryCode  value d-block d-sm-inline-block d-lg-block d-xl-inline-block "></span></li>
                                <li><span class="title">phone: </span><span class="InstructorNumber value d-block d-sm-inline-block d-lg-block d-xl-inline-block "></span></li>
                                <li><span class="title">Email: </span><span class="InstructorEmail  value d-block d-sm-inline-block d-lg-block d-xl-inline-block "></span></li>
                                <li><span class="title">Skype: </span><span class="value d-block d-sm-inline-block d-lg-block d-xl-inline-block " id="InstructorSkype"></span></li>
                                <li><span class="title">langages: </span><span class="value d-block d-sm-inline-block d-lg-block d-xl-inline-block " id="Instructorlangages"></span></li>
                            </ul>
                            <a class="button" id="downloadCVButton" download>
                                <span class="button-text">Download CV</span>
                                <span class="button-icon fa fa-download"></span>
                            </a>
                        </div>
                    </div>
                    @*----------------------------------------BindMasterproExtraInfo------------------------------*@
                    <div class="about-rt-col">
                        <div class="row-iner" id="BindMasterproExtradetailsDetail">

                        </div>
                    </div>
                </div>
