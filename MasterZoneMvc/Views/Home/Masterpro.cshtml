    <!-- contact us -->
    <div class="contact  tab-items" id="contact_tab">

        <div class="container " data-aos-duration="1200">
            <div class="title-section text-start text-sm-center " data-aos-duration="1200">
                <h1>get in <span>touch</span></h1>
            </div>
            <div class="row-flex row">
                <div class="row-flex-iner col-12 col-lg-4">
                    <h3 class="text-uppercase custom-title mb-0 ft-wt-600 pb-3">Don't be shy !</h3>
                    <p class="open-sans-font mb-4">Feel free to get in touch with me. I am always open to discussing new projects, creative ideas or opportunities to be part of your visions.</p>
                    <p class="open-sans-font custom-span-contact position-relative"><i class="fa fa-map position-absolute"></i><span class="d-block">Address Point</span><p class="InstructorAddress"></p></p>
                    <p class="open-sans-font custom-span-contact position-relative"><i class="fa fa-envelope-open position-absolute"></i><span class="d-block">mail me</span><p class="InstructorEmail"></p></p>
                    <p class="open-sans-font custom-span-contact position-relative"><i class="fa fa-phone-square position-absolute"></i><span class="d-block">call me</span><span class="InstructorNumber"></span></p>
                    <div class="social-follow">
                        <div class="social-icon">
                            <div class="InstructorFacebook "></div>
                        </div>
                        <div class="social-icon">
                            <div class="InstructorTwitter"></div>
                        </div>

                        <div class="social-icon">
                            <div class="InstructorInstagram"></div>
                        </div>
                        <div class="social-icon">
                            <div class="InstructorLinkdin"></div>
                        </div>
                    </div>
                </div>
                <div class="row-flex-in col-12 col-lg-8">
                    <form id="myForm" class="contactform">
                        <div class="row">
                            <div class="col-12 col-md-6">
                                <div class="form-group"><input type="text" name="name" id="text_Contact_Name" placeholder="YOUR NAME" required=""></div>
                                <span class="error-message" id="error_text_Contact_Name"></span>

                            </div>
                            <div class="col-12 col-md-6">
                                <div class="form-group"><input type="email" name="user_email" id="text_Email_Address" placeholder="YOUR EMAIL" required=""></div>
                                <span class="error-message" id="error_text_Email_Address"></span>

                            </div>
                            <div class="col-12 col-md-12">
                                <div class="form-group"><input type="text" name="subject" id="text_phone" placeholder="YOUR NUMBER" required=""></div>
                                <span class="error-message" id="error_text_phone"></span>

                            </div>

                            <div class="col-12">
                                <div class="form-group"><textarea name="message" id="text_Messages" placeholder="YOUR MESSAGE" required=""></textarea></div>
                                <span class="error-message" id="error_text_Messages"></span>

                            </div>
                            <div class="col-12">
                                <button type="button" class="button" onclick="btnSubmitUser()">
                                    <span class="button-text">Send Message</span>
                                    <span class="button-icon fa-regular fa-paper-plane"></span>
                                </button>
                            </div>
                        </div>

                    </form>

                </div>
            </div>
        </div>
    </div>

