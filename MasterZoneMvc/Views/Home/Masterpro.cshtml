<body>
    <style>
        .header {
            background-color: transparent;
        }
        img.inst-profileimage.InstructorProfileImage {
            width: 95%;
            margin: 10px 10px 10px 10px;
            max-height: 400px;
        }

        .profile-content .slide.lazyloaded {
            position: relative;
            box-shadow: 1px 4px 10px 1px;
            min-width: 400px;
            text-align: center;
        }

        .slide.lazyloaded {
            border: 10px solid #f3a762;
            margin: 90px 50px 50px 50px !important;
        }

        body .bg-curve:after {
            content: none !important;
        }

        p.InstructorName {
            text-align: center;
            font-weight: 700;
            font-size: 25px;
            margin-bottom: 10px;
        }

        .profile-content {
            margin-left: 30px;
        }

        .contact .contactform input[type=email], .contact .contactform input[type=text] {
            border: 1px solid #f3a762 !important;
            background: #fff !important;
            color: #f3a762 !important;
        }

        .contact .contactform textarea {
            border: 1px solid #f3a762 !important;
            background: #fff !important;
            color: #F3a762 !important;
        }

        .slide.lazyloaded button {
            margin: 0px auto;
            width: 50%;
            height: 40px;
            border: none;
            margin-bottom: 20px;
            border-radius: 5px;
            font-size: 20px;
        }

        a.message-button.button {
            padding: 15px 7px;
            width: 50%;
            border-radius: 25px;
            margin-bottom: 20px;
        }

        .tabs-main li {
            min-width: 110px;
            border: 1px solid #f3a762;
            padding: 5px 7px;
            border-radius: 5px;
        }

        .portfolio-section .port-tab {
            margin-top: 70px;
        }
        /*----------header start----------------*/
        @@media (min-width: 800px) {
            header.header {
                height: 105px !important;
            }
        }

        header.header {
            background: #f3a762;
            width: 100%;
            position: sticky;
            top: 50px !important;
            transition: .5s ease-in-out, padding .5s ease-in-out;
            z-index: 9;
        }

        .header .page-center {
            max-width: 1300px;
            margin: 0 auto;
            padding: 0 20px;
        }

        .header-main {
            display: flex;
            align-items: center;
            justify-content: space-between;
            padding: 10px 0;
        }

        .header .logo {
            flex: 0 0 180px;
            padding-right: 20px;
        }

            .header .logo a {
                display: flex;
                align-items: center;
            }

                .header .logo a img {
                    max-width: 40px;
                }

        .header span.logo-name {
            color: #000;
            font-weight: 600;
            font-size: 16px;
            padding-left: 10px;
        }

        .navigation-section-wrap {
            flex: 1;
            display: flex;
            justify-content: flex-end;
            position: relative;
        }

        ul {
            margin: 0;
            padding: 0;
            list-style: none;
        }

        .header .menu > ul > li {
            display: inline-block;
            margin: 0 7px;
        }

            .header .menu > ul > li > a {
                padding: 30px 0;
                display: inline-block;
                position: relative;
                color: var(--anchorColor);
                font-size: 16px;
                font-weight: 400;
            }

        /*-----------header end---------------*/
        /*-----------footer start---------------*/
        .footer.footer {
            padding: 50px 0;
            border-top: 1px solid #f3a762 !important;
            background: #f3a762;
            position:relative;
            z-index:99999;
        }

        @@media(min-width: 1200px) {
            .footer .container, .container-lg, .container-md, .container-sm, .container-xl {
                max-width: 1530px;
            }
        }
      
        .footer .row-sec {
            display: flex;
            flex-wrap: wrap;
        }

            .footer .row-sec > div {
                flex: 0 0 25%;
                max-width: 100%;
                padding-right: 30px;
            }

        .footer h4 {
            margin: 0 0 8px 0;
            color: var(--headingColor);
            font-size: 30px;
            font-family: 'Open Sans', sans-serif;
            font-weight: 400;
        }

        .small, small {
            font-size: 80%;
            font-weight: 400;
        }

        .footer p {
            margin: 0 0 8px 0;
            font-family: 'Open Sans', sans-serif;
            color: #333333;
            line-height: 1.5;
        }

        a {
            transition: all .3s;
            text-decoration: none;
            display: inline-block;
        }

        .footer .footer-social {
            display: flex;
            align-items: center;
            margin-top: 15px;
        }

        .footer .social-icon {
            margin: 0 10px 10px 0;
        }

            .footer .social-icon > a > i {
                color: #000;
                transition: all .4s;
            }

        .footer h5 {
            margin: 0 0 10px 0;
            color: var(--headingColor);
            font-size: 24px;
            font-family: 'Open Sans', sans-serif;
            font-weight: 500;
        }

        .footer ul {
            margin: 0;
            padding: 0;
            list-style: none;
        }

        .footer .menu-one ul > li > a {
            color: #fff;
            margin-bottom: 10px;
            transition: all .3s;
        }

        .footer p {
            margin: 0 0 8px 0;
            font-size: 18px;
            font-family: 'Open Sans', sans-serif;
            font-weight: 300;
            color: #333333;
            line-height: 1.5;
        }

        .footer .col-lg-two a {
            color: #000;
            display: block;
            margin-bottom: 10px;
        }

        .copyrights.border-top.pt-3 {
            background: #f3a762;
            overflow: hidden;
        }

      

        .copyrights.border-top.pt-3 strong.d-flex.copyrights-text {
            display: flex;
            align-items: center;
        }

        .copyrights.border-top.pt-3 a.copyright-company-name.mr-2.ml-2 {
            height: 32px;
        }

        @@media (min-width: 768px) {
            .float-md-right {
                float: right !important;
            }
        }

        .footer-logo.mb-3 img {
            width: 30%;
        }

        .footer-logo.mb-3 {
            display: flex;
            align-items: center;
        }

        .para_logo {
            color: white !important;
        }

        a.text-white {
            color: #fff !important;
        }

        .footer h5 {
            color: #fff;
        }

        .home .social-icon > a, .social-icon > a {
            background: none;
        }
        /*-----------footer end---------------*/
        body .bg-curve:before {
            display: none !important;
        }

        .footer .social-icon {
            margin: 0 10px 10px 0;
            border: 2px solid;
            padding: 5px;
            border-radius: 50%;
            background: #fff;
        }
        .position-fixed {
            position: fixed !important;
        }
    </style>
    <input type="hidden" id="hidden_businessOwnerLoginId" value="@ViewBag.BusinessOwnerLoginId">
