                <div class="my-services">
                    <div class="title-section text-start text-sm-center " data-aos-duration="1200">
                        <h1>My <span>Service</span></h1>
                    </div>
                    <div class="col-iner-main" id="BindBusinessServiceSingleDetail">

                    </div>
                </div>
                <!--------------------------------- Certificate ----------------------------------------->
                <div class="certificates-section" id="certificates">
                    <div class="title-section text-start text-sm-center " data-aos-duration="1200">
                        <h1>My <span>certificates</span></h1>
                    </div>
                    <div class="certi-main " id="BindBusinessCertificateDetail">
                    </div>
                </div>
