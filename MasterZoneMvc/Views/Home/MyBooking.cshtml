    <link rel="stylesheet" href="~/Content/assets/conatiner-style.css">
    <link rel="stylesheet" href="~/Content/assets/Style_myBooking.css">
    <style>
        img.ls-p-lg.mblg-ls {
            box-shadow: none !important;
        }
        p.successbtn {
            text-align: center;
            padding: 4px;
            font-size: 14px;
            background-color: green;
            color: #fff;
            width: 75%;
            margin-top: 12px;
        }
        p.expirebtn {
            text-align: center;
            padding: 4px;
            font-size: 14px;
            background-color: red;
            color: #fff;
            width: 75%;
            margin-top: 12px;
        }

        p.pendingbtn {
            text-align: center;
            padding: 4px;
            font-size: 14px;
            background-color: orange;
            color: #fff;
            width: 75%;
            margin-top: 12px;
        }

        p.upcomingbtn {
            text-align: center;
            padding: 5px;
            font-size: 14px;
            background-color: #6eadfb;
            color: white;
        }
        .edbt{
            display :flex;
        }
        a.btn.btn-sm.ml-4.buyagainbtn {
            color: #fff;
            background-color: #007bff;
        }
    </style>


}
<input type="hidden" id="hidden_businessOwnerLoginId" value="@ViewBag.BusinessOwnerLoginId">

<section id="section-mc1" class="section-mk1">
    <div class="container">
        <div class="row">

            <div class="col-sm-3">
                @Html.Partial("_VisitorPanelSideBarPartial")
