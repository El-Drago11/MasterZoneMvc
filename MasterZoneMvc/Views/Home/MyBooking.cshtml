                                                    <tr>
                                                        <th>OrederId</th>
                                                        <th>Class Name</th>
                                                        <th>Price</th>
                                                        <th>Date</th>
                                                        <th>Status</th>
                                                        <th>Action</th>
                                                    </tr>
                                                </thead>
                                            </table>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>

                        @*----------------------Event booking---------------------------*@

                        <div id="fav_Event" class="tab-pane fade p-3" role="tabpanel">
