                                <div class="col-12 mb-4">
                                    <div class="card">
                                        <div class="card-body">
                                            <table id="tblEventBookingDetails" class="Commontable table-bordered"  data-order='[[ 0, "desc" ]]'>
                                                <thead>
                                                    <tr>
                                                        <th>Sno.</th>
                                                        <th>OrderId</th>
                                                        <th>Name</th>
                                                        <th>Price</th>
                                                        <th>Date</th>
                                                        <th>Status</th>
