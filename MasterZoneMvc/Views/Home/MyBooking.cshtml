            <div class="col-sm-9">

                <!-- My-classes-section -->

                <div id="My-Classes-i" class="section-1-my-classes inrsection-mltp">
                    <h2 class="mk-hdffjsdkfj">My Purchase</h2>

                    <ul class="nav nav-tabs custom-tab-bar mtab-3jrjfj">
                        <li id="upc-fhgjfie" class="nav-item">
