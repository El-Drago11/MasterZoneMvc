            }

            div#tblTrainingBookingDetails_wrapper {
                width: 100%;
                overflow-x: scroll;
            }

            div#tblPlanBookingDetails_wrapper {
                width: 100%;
                overflow-x: scroll;
            }
        }
        a.btn.btn-sm.ml-4.ViewMoreDetail {
            color: white;
            background-color: #f3a762;
        }
