            $('.tab-section.desktop .tabs>li').click(function () {
                // $('.tab-section.desktop .tabbing').hide();
                $('.tab-section.desktop .tabs li.active').removeClass('active');
                $(this).addClass('active');

                var panel = $(this).attr('data-tab');
                $(panel).fadeIn(1000);

                return false;  // prevents link action

            });  // end click

            $('.tab-section.desktop .tabs li:first').click();


            // mobile
            $('.tab-section.mobile .tabs>li').click(function () {
                $('.tab-section.mobile .tabbing').hide();
                $('.tab-section.mobile .tabs li.active').removeClass('active');
                $(this).addClass('active');

                var panel = $(this).attr('data-tab');
                $(panel).fadeIn(1000);

                return false;  // prevents link action

            });  // end click

            $('.tab-section.mobile .tabs li:first').click();

            // men slider


            // end
        });

