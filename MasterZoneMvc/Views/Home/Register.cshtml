                </div>
                <!-- END SIGN IN -->
            </div>
            <!-- END FORM SECTION -->
            <!-- CONTENT SECTION -->
            <div class="row content-row">
                <!-- SIGN IN CONTENT -->
                <div id="topsc-2" class="col align-items-center flex-col">
                    <div class="d-md-none">
                        <a href="/"><img class="fm-lg" src="@Resources.StaticResources.MasterzoneLogo"></a>
                    </div>
                    <div class="text sign-in d-md-none">
                        <h2>
                            Join with us
                        </h2>

                    </div>
                    <div class="img sign-in">

                    </div>
                </div>
                <!-- END SIGN IN CONTENT -->
                <!-- SIGN UP CONTENT -->
                <div class="col align-items-center flex-col">
                    <div class="img sign-up">

                    </div>
                    <div class="text sign-up d-none d-md-block">
                        <h2>
                            Join with us
                        </h2>
                    </div>
                </div>
                <!-- END SIGN UP CONTENT -->
            </div>
            <!-- END CONTENT SECTION -->
        </div>
    </seciton>

    <script>
        let container = document.getElementById('container')

        toggle = () => {
            container.classList.toggle('sign-in')
            container.classList.toggle('sign-up')
        }

        setTimeout(() => {
            container.classList.add('sign-in')
        }, 200);
    </script>

    <!-- Code injected by live-server -->
